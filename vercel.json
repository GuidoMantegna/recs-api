{
  "version": 3,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/output/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ]
}
